[Appearance]
AntiAliasFonts=true
BoldIntense=false
ColorScheme=OneDark
Font=FiraCode Nerd Font,10,-1,5,50,0,0,0,0,0,Regular
UseFontLineChararacters=true

[Cursor Options]
CursorShape=0

[General]
BrowserApplication[$e]=!google-chrome
Environment=TERM=xterm-256color,COLORTERM=truecolor
Name=OneDark
Parent=FALLBACK/
ShowTerminalSizeHint=false
TerminalColumns=165
TerminalMargin=5
TerminalRows=42

[Interaction Options]
AutoCopySelectedText=true
TrimTrailingSpacesInSelectedText=true
UnderlineFilesEnabled=true

[Scrolling]
HistoryMode=1
ScrollBarPosition=2

[Terminal Features]
BlinkingCursorEnabled=false
BlinkingTextEnabled=false
UrlHintsModifiers=67108864

